@page
@model tbkk.Pages.NewFolder.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>OT</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="OT.OTID" />
            <div class="form-group">
                <label asp-for="OT.TimeStart" class="control-label"></label>
                <input asp-for="OT.TimeStart" class="form-control" />
                <span asp-validation-for="OT.TimeStart" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OT.TimeEnd" class="control-label"></label>
                <input asp-for="OT.TimeEnd" class="form-control" />
                <span asp-validation-for="OT.TimeEnd" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OT.date" class="control-label"></label>
                <input asp-for="OT.date" class="form-control" />
                <span asp-validation-for="OT.date" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OT.TypeOT" class="control-label"></label>
                <input asp-for="OT.TypeOT" class="form-control" />
                <span asp-validation-for="OT.TypeOT" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OT.TypStatus" class="control-label"></label>
                <input asp-for="OT.TypStatus" class="form-control" />
                <span asp-validation-for="OT.TypStatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="OT.OT_CompanyID" class="control-label"></label>
                <select asp-for="OT.OT_CompanyID" class="form-control" asp-items="ViewBag.OT_CompanyID"></select>
                <span asp-validation-for="OT.OT_CompanyID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="./Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
